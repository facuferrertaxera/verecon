{
  "_version": "1.76.0",
  "sap.app": {
    "id": "tech.taxera.taxreporting.verecon",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "f36cf9a9-6f69-4326-be52-f3794bd37103"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/TAXERA/VERECON_UI_O2/",
        "type": "OData",
        "settings": {
          "annotations": [
            "TAXERA_VERECON_UI_O2_VAN"
          ],
          "localUri": "annotations/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "TAXERA_VERECON_UI_O2_VAN": {
        "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='%2FTAXERA%2FVERECON_UI_O2_VAN',Version='0001')/$value/",
        "type": "ODataAnnotation",
        "settings": {
          "localUri": "annotations/annotation.xml"
        }
      }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "vr_ecsl-reconcile": {
          "semanticObject": "vr_ecsl",
          "action": "reconcile",
          "title": "{{vr_ecsl-reconcile.flpTitle}}",
          "info": "{{vr_ecsl-reconcile.flpInfo}}",
          "icon": "sap-icon://compare-2",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "fullWidth": true,
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.143.2",
        "libs": {
          "sap.m": {},
          "sap.ui.core": {},
          "sap.f": {},
          "sap.ui.comp": {},
          "sap.ui.table": {},
          "sap.suite.ui.microchart": {},
          "sap.viz": {}
        }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "tech.taxera.taxreporting.verecon.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "tech.taxera.taxreporting.verecon.view",
        "async": true,
        "viewPath": "tech.taxera.taxreporting.verecon.view"
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        },
        {
          "name": "ReconciliationDetail",
          "pattern": "reconciliation/{reconId}",
          "target": [
            "TargetReconciliationDetail"
          ]
        }
      ],
      "targets": {
        "TargetMain": {
          "id": "Main",
          "name": "Main"
        },
        "TargetReconciliationDetail": {
          "id": "ReconciliationDetail",
          "name": "ReconciliationDetail"
        }
      }
    },
    "rootView": {
      "viewName": "tech.taxera.taxreporting.verecon.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
