<mvc:View controllerName="tech.taxera.taxreporting.verecon.controller.ReconciliationDetail"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:smartform="sap.ui.comp.smartform"
    xmlns:smartField="sap.ui.comp.smartfield"
    xmlns:form="sap.ui.layout.form"
    xmlns:f="sap.f"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    xmlns:cssgrid="sap.ui.layout.cssgrid"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:st="sap.ui.comp.smarttable"
    xmlns:sv="sap.ui.comp.smartvariants"
    xmlns:smartFilterBar="sap.ui.comp.smartfilterbar" 
    xmlns:table="sap.ui.table"
    xmlns:m="sap.m"
    xmlns:smartVariant="sap.ui.comp.smartvariants.SmartVariantManagement"
    xmlns:mc="sap.suite.ui.microchart"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    xmlns:viz="sap.viz.ui5.controls"
    xmlns:viz.data="sap.viz.ui5.data"
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds">
    <f:DynamicPage headerExpanded="{view>/reconciliationDetail/headerExpanded}" toggleHeaderOnTitleClick="{view>/reconciliationDetail/titleClickable}" class="sapUiNoContentPadding" height="100%">
        <!-- DynamicPage Header -->
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <f:content>
                    <!-- Charts for reconciliation overview -->
                    <cssgrid:CSSGrid
                        gridTemplateColumns="repeat(12, 1fr)"
                        gridGap="1.5rem"
                        width="100%"
                        class="sapUiNoContentPadding sapUiSmallMarginBeginEnd"
                        visible="{view>/reconciliationDetail/chartFiltersVisible}">
                        
                        <!-- Reconciliation Details Card -->
                        <f:Card class="sapUiNoContentPadding" height="220px">
                            <f:layoutData>
                                <cssgrid:GridItemLayoutData gridColumn="span 3" />
                            </f:layoutData>
                            <f:content>
                                <VBox class="sapUiMediumMargin" width="100%">
                                    <Title text="{i18n>reconciliationDetail.ReconciliationDetails}" titleStyle="H4" class="sapUiSmallMarginBottom" />
                                    <HBox width="100%" justifyContent="SpaceBetween" alignItems="Center" class="sapUiSmallMarginBottom">
                                        <Label text="{i18n>reconciliationList.CountryList}" design="Bold" />
                                        <VBox width="100%" class="sapUiTinyMarginBegin" alignItems="End">
                                            <Tokenizer id="reconciliationCountryListTokenizer" editable="false" width="100%" multiLine="true"/>
                                        </VBox>
                                    </HBox>
                                    <HBox width="100%" justifyContent="SpaceBetween" alignItems="Center" class="sapUiSmallMarginBottom">
                                        <Label text="{i18n>reconciliationList.CompanyCodeList}" design="Bold" />
                                        <VBox width="100%" class="sapUiTinyMarginBegin" alignItems="End">
                                            <Tokenizer id="reconciliationCompanyCodeListTokenizer" editable="false" width="100%" multiLine="true"/>
                                        </VBox>
                                    </HBox>
                                    <HBox width="100%" justifyContent="SpaceBetween" alignItems="End">
                                        <Label text="{i18n>reconciliationDetail.DateRange}" design="Bold" />
                                        <Text text="{path: 'FromDate', type: '.types.TaxeraDateType'} - {path: 'ToDate', type: '.types.TaxeraDateType'}" class="sapUiTinyMarginBegin"/>
                                    </HBox>
                                </VBox>
                            </f:content>
                        </f:Card>
                        
                        <!-- Company Code Differences Treemap Card -->
                        <f:Card class="sapUiNoContentPadding" height="220px">
                            <f:layoutData>
                                <cssgrid:GridItemLayoutData gridColumn="span 3" />
                            </f:layoutData>
                            <f:content>
                                <viz:VizFrame 
                                    id="companyCodeTreemap"
                                    uiConfig="{applicationSet:'fiori'}"
                                    height="100%" 
                                    width="100%" 
                                    vizType="treemap"
                                    legendVisible="false">
                                    <viz:dataset>
                                        <viz.data:FlattenedDataset data="{view>/reconciliationDetail/treemapData/companyCodes}">
                                            <viz.data:dimensions>
                                                <viz.data:DimensionDefinition name="CompanyCode" value="{CompanyCode}" />
                                            </viz.data:dimensions>
                                            <viz.data:measures>
                                                <viz.data:MeasureDefinition name="Differences" value="{DiffGrossAmount}" />
                                            </viz.data:measures>
                                        </viz.data:FlattenedDataset>
                                    </viz:dataset>
                                    <viz:feeds>
                                        <viz.feeds:FeedItem uid="color" type="Measure" values="Differences" />
                                        <viz.feeds:FeedItem uid="weight" type="Measure" values="Differences" />
                                        <viz.feeds:FeedItem uid="title" type="Dimension" values="CompanyCode" />
                                    </viz:feeds>
                                    <viz:dependents>
                                        <viz:Popover id="companyCodeTreemapPopover"></viz:Popover>
                                    </viz:dependents>
                                </viz:VizFrame>
                            </f:content>
                        </f:Card>
                        
                        <!-- Tax Code Differences Treemap Card -->
                        <f:Card class="sapUiNoContentPadding" height="220px">
                            <f:layoutData>
                                <cssgrid:GridItemLayoutData gridColumn="span 4" />
                            </f:layoutData>
                            <f:content>
                                <viz:VizFrame 
                                    id="taxCodeTreemap"
                                    uiConfig="{applicationSet:'fiori'}"
                                    height="100%" 
                                    width="100%" 
                                    vizType="treemap"
                                    legendVisible="false">
                                    <viz:dataset>
                                        <viz.data:FlattenedDataset data="{view>/reconciliationDetail/treemapData/taxCodes}">
                                            <viz.data:dimensions>
                                                <viz.data:DimensionDefinition name="TaxCode" value="{TaxCode}" />
                                            </viz.data:dimensions>
                                            <viz.data:measures>
                                                <viz.data:MeasureDefinition name="Differences" value="{DiffGrossAmount}" />
                                            </viz.data:measures>
                                        </viz.data:FlattenedDataset>
                                    </viz:dataset>
                                    <viz:feeds>
                                        <viz.feeds:FeedItem uid="color" type="Measure" values="Differences" />
                                        <viz.feeds:FeedItem uid="weight" type="Measure" values="Differences" />
                                        <viz.feeds:FeedItem uid="title" type="Dimension" values="TaxCode" />
                                    </viz:feeds>
                                    <viz:dependents>
                                        <viz:Popover id="taxCodeTreemapPopover"></viz:Popover>
                                    </viz:dependents>
                                </viz:VizFrame>
                            </f:content>
                        </f:Card>
                        
                        <!-- Total Difference Card -->
                        <f:Card height="220px" press="onTotalDifferenceCardPress" semanticRole="ListItem" class="{= ${view>/reconciliationDetail/showOnlyDifferences} ? 'totalDifferenceCardActive' : ''}">
                            <f:layoutData>
                                <cssgrid:GridItemLayoutData gridColumn="span 2" />
                            </f:layoutData>
                            <f:content>
                                <VBox width="100%" height="100%" class="sapUiSmallMargin">
                                    <Title text="{i18n>reconciliationDetail.TotalDifference}" titleStyle="H6" class="sapUiTinyMarginBottom" />
                                    <VBox justifyContent="Center" alignItems="Center" width="100%" height="100%" class="sapUiTinyMarginTop">
                                        <NumericContent 
                                            value="{path: 'view>/reconciliationDetail/totalDifference/value', formatter: '.formatter.formatNumberWithScale'}" 
                                            scale=""
                                            valueColor="Error"
                                            class="totalDifferenceNumericContent" />
                                    </VBox>
                                </VBox>
                            </f:content>
                        </f:Card>
                        
                    </cssgrid:CSSGrid>
                </f:content>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <st:SmartTable
                id="documentsSmartTable"
                entitySet="Document"
                tableBindingPath="to_Document"
                tableType="Table"
                useExportToExcel="true"
                useVariantManagement="false"
                enableAutoBinding="false"
                useTablePersonalisation="true"
                showRowCount="true"
                showTablePersonalisation="true"
                initialise="true"
                beforeRebindTable="onBeforeRebindDocumentsTable"
                requestAtLeastFields="CompanyCode,DocumentNo,PostingDate,TaxCode,Currencycode,VatrGrossAmount,EcslGrossAmount,DiffGrossAmount,Status,StatusText"
                class="sapUiNoContentPadding"
                height="100%">
                    <st:customToolbar>
                        <OverflowToolbar design="Transparent">
                            <Title text="{i18n>reconciliationDetail.Documents}" titleStyle="H3"/>
                            <ToolbarSpacer />
                        </OverflowToolbar>
                    </st:customToolbar>
                    <table:Table
                        id="documentsTable"
                        enableColumnFreeze="true"
                        visibleRowCountMode="Auto"
                        selectionMode="None">
                <table:columns>
                    <table:Column width="8rem" sortProperty="CompanyCode" filterProperty="CompanyCode" hAlign="Left">
                        <table:customData>
                            <core:CustomData key="p13nData" value='\{"columnKey": "CompanyCode", "leadingProperty": "CompanyCode", "sortProperty": "CompanyCode", "filterProperty": "CompanyCode", "columnIndex": 0}'/>
                        </table:customData>
                        <table:label>
                            <m:Label text="{i18n>reconciliationDetail.CompanyCode}" visible="false"/>
                        </table:label>
                        <table:multiLabels>
                            <m:Label visible="false"/>
                            <m:Label text="{i18n>reconciliationDetail.CompanyCode}" textAlign="Left" width="100%"/>
                        </table:multiLabels>
                        <table:template>
                            <m:Text text="{CompanyCode}" textAlign="Left"/>
                        </table:template>
                    </table:Column>
                    <table:Column width="10rem" sortProperty="DocumentNo" filterProperty="DocumentNo" hAlign="Left">
                        <table:customData>
                            <core:CustomData key="p13nData" value='\{"columnKey": "DocumentNo", "leadingProperty": "DocumentNo", "sortProperty": "DocumentNo", "filterProperty": "DocumentNo", "columnIndex": 1}'/>
                        </table:customData>
                        <table:label>
                            <m:Label text="{i18n>reconciliationDetail.DocumentNo}" visible="false"/>
                        </table:label>
                        <table:multiLabels>
                            <m:Label visible="false"/>
                            <m:Label text="{i18n>reconciliationDetail.DocumentNo}" textAlign="Left" width="100%"/>
                        </table:multiLabels>
                        <table:template>
                            <m:Text text="{DocumentNo}" textAlign="Left"/>
                        </table:template>
                    </table:Column>
                    <table:Column width="6rem" sortProperty="FiscalYear" filterProperty="FiscalYear" visible="false">
                        <table:customData>
                            <core:CustomData key="p13nData" value='\{"columnKey": "FiscalYear", "leadingProperty": "FiscalYear", "sortProperty": "FiscalYear", "filterProperty": "FiscalYear", "columnIndex": 8}'/>
                        </table:customData>
                        <table:label>
                            <m:Label text="{i18n>reconciliationDetail.FiscalYear}" visible="false"/>
                        </table:label>
                        <table:multiLabels>
                            <m:Label visible="false"/>
                            <m:Label text="{i18n>reconciliationDetail.FiscalYear}" textAlign="Center" width="100%"/>
                        </table:multiLabels>
                        <table:template>
                            <m:Text text="{FiscalYear}"/>
                        </table:template>
                    </table:Column>
                    <table:Column width="10rem" sortProperty="PostingDate" filterProperty="PostingDate" hAlign="Left">
                        <table:customData>
                            <core:CustomData key="p13nData" value='\{"columnKey": "PostingDate", "leadingProperty": "PostingDate", "sortProperty": "PostingDate", "filterProperty": "PostingDate", "columnIndex": 2}'/>
                        </table:customData>
                        <table:label>
                            <m:Label text="{i18n>reconciliationDetail.PostingDate}" visible="false"/>
                        </table:label>
                        <table:multiLabels>
                            <m:Label visible="false"/>
                            <m:Label text="{i18n>reconciliationDetail.PostingDate}" textAlign="Left" width="100%"/>
                        </table:multiLabels>
                        <table:template>
                            <m:Text text="{path: 'PostingDate', type: 'sap.ui.model.type.Date', formatOptions: {style: 'medium'}}" textAlign="Left"/>
                        </table:template>
                    </table:Column>
                    <table:Column width="8rem" sortProperty="TaxCode" filterProperty="TaxCode" hAlign="Left">
                        <table:customData>
                            <core:CustomData key="p13nData" value='\{"columnKey": "TaxCode", "leadingProperty": "TaxCode", "sortProperty": "TaxCode", "filterProperty": "TaxCode", "columnIndex": 3}'/>
                        </table:customData>
                        <table:label>
                            <m:Label text="{i18n>reconciliationDetail.TaxCode}" visible="false"/>
                        </table:label>
                        <table:multiLabels>
                            <m:Label visible="false"/>
                            <m:Label text="{i18n>reconciliationDetail.TaxCode}" textAlign="Left" width="100%"/>
                        </table:multiLabels>
                        <table:template>
                            <m:Text text="{TaxCode}" textAlign="Left"/>
                        </table:template>
                    </table:Column>
                    <table:Column width="6rem" sortProperty="Currencycode" filterProperty="Currencycode" visible="false">
                        <table:customData>
                            <core:CustomData key="p13nData" value='\{"columnKey": "Currencycode", "leadingProperty": "Currencycode", "sortProperty": "Currencycode", "filterProperty": "Currencycode", "columnIndex": 9}'/>
                        </table:customData>
                        <table:label>
                            <m:Label text="{i18n>reconciliationDetail.Currency}" visible="false"/>
                        </table:label>
                        <table:multiLabels>
                            <m:Label visible="false"/>
                            <m:Label text="{i18n>reconciliationDetail.Currency}" textAlign="Center" width="100%"/>
                        </table:multiLabels>
                        <table:template>
                            <m:Text text="{Currencycode}"/>
                        </table:template>
                    </table:Column>
                    <!-- VATR Column Group - spans 3 columns -->
                    <table:Column width="8rem" sortProperty="VatrBaseAmount" filterProperty="VatrBaseAmount" headerSpan="3,2" visible="false" hAlign="End">
                        <table:customData>
                            <core:CustomData key="p13nData" value='\{"columnKey": "VatrBaseAmount", "leadingProperty": "VatrBaseAmount", "sortProperty": "VatrBaseAmount", "filterProperty": "VatrBaseAmount", "columnIndex": 10, "type": "numeric"}'/>
                        </table:customData>
                        <table:label>
                            <m:Label text="{i18n>reconciliationDetail.BaseAmount}" visible="false"/>
                        </table:label>
                        <table:multiLabels>
                            <m:Label text="{i18n>reconciliationDetail.VATR}" textAlign="Center" width="100%"/>
                            <m:Label text="{i18n>reconciliationDetail.BaseAmount}" textAlign="Center" width="100%"/>
                        </table:multiLabels>
                        <table:template>
                            <m:ObjectNumber number="{VatrBaseAmount}" unit="{Currencycode}" textAlign="End"/>
                        </table:template>
                    </table:Column>
                    <table:Column width="8rem" sortProperty="VatrTaxAmount" filterProperty="VatrTaxAmount" headerSpan="2" visible="false">
                        <table:customData>
                            <core:CustomData key="p13nData" value='\{"columnKey": "VatrTaxAmount", "leadingProperty": "VatrTaxAmount", "sortProperty": "VatrTaxAmount", "filterProperty": "VatrTaxAmount", "columnIndex": 11, "type": "numeric"}'/>
                        </table:customData>
                        <table:label>
                            <m:Label text="{i18n>reconciliationDetail.TaxAmount}" visible="false"/>
                        </table:label>
                        <table:multiLabels>
                            <m:Label text="{i18n>reconciliationDetail.VATR}"/>
                            <m:Label text="{i18n>reconciliationDetail.TaxAmount}" textAlign="Center" width="100%"/>
                        </table:multiLabels>
                        <table:template>
                            <m:ObjectNumber number="{VatrTaxAmount}" unit="{Currencycode}"/>
                        </table:template>
                    </table:Column>
                    <table:Column width="8rem" sortProperty="VatrGrossAmount" filterProperty="VatrGrossAmount" hAlign="End">
                        <table:customData>
                            <core:CustomData key="p13nData" value='\{"columnKey": "VatrGrossAmount", "leadingProperty": "VatrGrossAmount", "sortProperty": "VatrGrossAmount", "filterProperty": "VatrGrossAmount", "columnIndex": 4, "type": "numeric"}'/>
                        </table:customData>
                        <table:label>
                            <m:Label text="{i18n>reconciliationDetail.GrossAmount}" visible="false"/>
                        </table:label>
                        <table:multiLabels>
                            <m:Label text="{i18n>reconciliationDetail.VATR}"/>
                            <m:Label text="{i18n>reconciliationDetail.GrossAmount}" textAlign="Center" width="100%"/>
                        </table:multiLabels>
                        <table:template>
                            <m:ObjectNumber number="{VatrGrossAmount}" unit="{Currencycode}" textAlign="End"/>
                        </table:template>
                    </table:Column>
                    <!-- ECSL Column Group - spans 3 columns -->
                    <table:Column width="8rem" sortProperty="EcslBaseAmount" filterProperty="EcslBaseAmount" headerSpan="3,2" visible="false" hAlign="End">
                        <table:customData>
                            <core:CustomData key="p13nData" value='\{"columnKey": "EcslBaseAmount", "leadingProperty": "EcslBaseAmount", "sortProperty": "EcslBaseAmount", "filterProperty": "EcslBaseAmount", "columnIndex": 12, "type": "numeric"}'/>
                        </table:customData>
                        <table:label>
                            <m:Label text="{i18n>reconciliationDetail.BaseAmount}" visible="false"/>
                        </table:label>
                        <table:multiLabels>
                            <m:Label text="{i18n>reconciliationDetail.ECSL}" textAlign="Center" width="100%"/>
                            <m:Label text="{i18n>reconciliationDetail.BaseAmount}" textAlign="Center" width="100%"/>
                        </table:multiLabels>
                        <table:template>
                            <m:ObjectNumber number="{EcslBaseAmount}" unit="{Currencycode}" textAlign="End"/>
                        </table:template>
                    </table:Column>
                    <table:Column width="8rem" sortProperty="EcslTaxAmount" filterProperty="EcslTaxAmount" headerSpan="2" visible="false">
                        <table:customData>
                            <core:CustomData key="p13nData" value='\{"columnKey": "EcslTaxAmount", "leadingProperty": "EcslTaxAmount", "sortProperty": "EcslTaxAmount", "filterProperty": "EcslTaxAmount", "columnIndex": 13, "type": "numeric"}'/>
                        </table:customData>
                        <table:label>
                            <m:Label text="{i18n>reconciliationDetail.TaxAmount}" visible="false"/>
                        </table:label>
                        <table:multiLabels>
                            <m:Label text="{i18n>reconciliationDetail.ECSL}"/>
                            <m:Label text="{i18n>reconciliationDetail.TaxAmount}" textAlign="Center" width="100%"/>
                        </table:multiLabels>
                        <table:template>
                            <m:ObjectNumber number="{EcslTaxAmount}" unit="{Currencycode}"/>
                        </table:template>
                    </table:Column>
                    <table:Column width="8rem" sortProperty="EcslGrossAmount" filterProperty="EcslGrossAmount" hAlign="End">
                        <table:customData>
                            <core:CustomData key="p13nData" value='\{"columnKey": "EcslGrossAmount", "leadingProperty": "EcslGrossAmount", "sortProperty": "EcslGrossAmount", "filterProperty": "EcslGrossAmount", "columnIndex": 5, "type": "numeric"}'/>
                        </table:customData>
                        <table:label>
                            <m:Label text="{i18n>reconciliationDetail.GrossAmount}" visible="false"/>
                        </table:label>
                        <table:multiLabels>
                            <m:Label text="{i18n>reconciliationDetail.ECSL}"/>
                            <m:Label text="{i18n>reconciliationDetail.GrossAmount}" textAlign="Center" width="100%"/>
                        </table:multiLabels>
                        <table:template>
                            <m:ObjectNumber number="{EcslGrossAmount}" unit="{Currencycode}" textAlign="End"/>
                        </table:template>
                    </table:Column>
                    <!-- Differences Column Group - spans 3 columns -->
                    <table:Column width="8rem" sortProperty="DiffBaseAmount" filterProperty="DiffBaseAmount" headerSpan="3,2" visible="false" hAlign="End">
                        <table:customData>
                            <core:CustomData key="p13nData" value='\{"columnKey": "DiffBaseAmount", "leadingProperty": "DiffBaseAmount", "sortProperty": "DiffBaseAmount", "filterProperty": "DiffBaseAmount", "columnIndex": 14, "type": "numeric"}'/>
                        </table:customData>
                        <table:label>
                            <m:Label text="{i18n>reconciliationDetail.BaseAmount}" visible="false"/>
                        </table:label>
                        <table:multiLabels>
                            <m:Label text="{i18n>reconciliationDetail.Differences}" textAlign="Center" width="100%"/>
                            <m:Label text="{i18n>reconciliationDetail.BaseAmount}" textAlign="Center" width="100%"/>
                        </table:multiLabels>
                        <table:template>
                            <m:ObjectNumber number="{DiffBaseAmount}" unit="{Currencycode}" state="{path: 'DiffBaseAmount', formatter: '.formatter.formatDiffAmountState'}" textAlign="End"/>
                        </table:template>
                    </table:Column>
                    <table:Column width="8rem" sortProperty="DiffTaxAmount" filterProperty="DiffTaxAmount" headerSpan="2" visible="false">
                        <table:customData>
                            <core:CustomData key="p13nData" value='\{"columnKey": "DiffTaxAmount", "leadingProperty": "DiffTaxAmount", "sortProperty": "DiffTaxAmount", "filterProperty": "DiffTaxAmount", "columnIndex": 15, "type": "numeric"}'/>
                        </table:customData>
                        <table:label>
                            <m:Label text="{i18n>reconciliationDetail.TaxAmount}" visible="false"/>
                        </table:label>
                        <table:multiLabels>
                            <m:Label text="{i18n>reconciliationDetail.Differences}"/>
                            <m:Label text="{i18n>reconciliationDetail.TaxAmount}" textAlign="Center" width="100%"/>
                        </table:multiLabels>
                        <table:template>
                            <m:ObjectNumber number="{DiffTaxAmount}" unit="{Currencycode}" state="{path: 'DiffTaxAmount', formatter: '.formatter.formatDiffAmountState'}"/>
                        </table:template>
                    </table:Column>
                    <table:Column width="8rem" sortProperty="DiffGrossAmount" filterProperty="DiffGrossAmount" hAlign="End">
                        <table:customData>
                            <core:CustomData key="p13nData" value='\{"columnKey": "DiffGrossAmount", "leadingProperty": "DiffGrossAmount", "sortProperty": "DiffGrossAmount", "filterProperty": "DiffGrossAmount", "columnIndex": 6, "type": "numeric"}'/>
                        </table:customData>
                        <table:label>
                            <m:Label text="{i18n>reconciliationDetail.GrossAmount}" visible="false"/>
                        </table:label>
                        <table:multiLabels>
                            <m:Label text="{i18n>reconciliationDetail.Differences}"/>
                            <m:Label text="{i18n>reconciliationDetail.GrossAmount}" textAlign="Center" width="100%"/>
                        </table:multiLabels>
                        <table:template>
                            <m:ObjectNumber number="{DiffGrossAmount}" unit="{Currencycode}" state="{path: 'DiffGrossAmount', formatter: '.formatter.formatDiffAmountState'}" textAlign="End"/>
                        </table:template>
                    </table:Column>
                    <!-- Status Column -->
                    <table:Column width="8rem" sortProperty="Status" filterProperty="Status" hAlign="Left">
                        <table:customData>
                            <core:CustomData key="p13nData" value='\{"columnKey": "Status", "leadingProperty": "Status", "sortProperty": "Status", "filterProperty": "Status", "columnIndex": 7}'/>
                        </table:customData>
                        <table:label>
                            <m:Label text="{i18n>reconciliationDetail.Status}" visible="false"/>
                        </table:label>
                        <table:multiLabels>
                            <m:Label visible="false"/>
                            <m:Label text="{i18n>reconciliationDetail.Status}" textAlign="Left" width="100%"/>
                        </table:multiLabels>
                        <table:template>
                            <m:ObjectStatus 
                                text="{StatusText}" 
                                state="{path: 'Status', formatter: '.formatter.formatStatusColor'}"
                                icon="{path: 'Status', formatter: '.formatter.formatStatusIcon'}"/>
                        </table:template>
                    </table:Column>
                </table:columns>
                    </table:Table>
                </st:SmartTable>
        </f:content>
    </f:DynamicPage>
</mvc:View>
